<p class="general-address">
<b><%= address.name %></b><br>
<%= address.address_line1 %><br>
<% if address.address_line2 != '' %>
  <%= address.address_line2 %><br>
<% end %>
<%= address.city %>, <%= address.state %><br>
<%= address.zip %>
<% if address.default %>
  <br>DEFAULT ADDRESS
<% end %>
</p>